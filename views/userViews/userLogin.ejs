      @font-face {
        font-family: 'Rochester';
        src: url('/fonts/rochester.woff2') format('woff2'),
             url('/fonts/rochester.woff') format('woff');
        font-weight: normal;
        font-style: normal;
        /* Optionally, you can include font-display and other properties here */
      }
      /* Apply the Rochester font to a specific CSS selector */
      .rochester-font {
        font-family: 'Rochester', sans-serif;
      }
      #ayra {
        color: palevioletred;
      }
      /* .btn{
        outline-color: palevioletred;;
        border: rgb(152, 7, 55);
