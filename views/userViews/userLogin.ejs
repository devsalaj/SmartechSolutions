      .no-arrows::-webkit-inner-spin-button,
.no-arrows::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

.no-arrows {
  -moz-appearance: textfield;
}

/* For Firefox */
.no-arrows[type=number] {
  -moz-appearance: textfield;
}

/* For Internet Explorer and Edge */
.no-arrows::-ms-clear {
  display: none;
}

/* Additional CSS to adjust input width */
.w-50 {
  width: 50%;
}

      #header-link {
    text-decoration: none;
    color: #000; /* Replace with your desired color */
  }
  h6,svg{
    color: palevioletred;
  }
#wishlist {
  background-color: rgb(255, 240, 244);
  border:solid;
  border-color: palevioletred;
}
.name-container {
  width: 200px; /* Adjust the width as per your requirement */
  overflow: hidden;
}

.name {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; /* Set the desired number of lines */
  text-overflow: ellipsis;
  overflow: hidden;
}
.image-zoom-cursor {
  background-color: transparent;
  border: none;
  outline: none;
  cursor: none;
}

    </style>
    
  </head>
  <body class="bg-gradient bg-primary-subtle ">
<section class="vh-100 gradient-custom mt-5">
  <div class="container mt-md-5 align-items-center ">
    <div class="row d-flex justify-content-center align-items-center">
      <div class="col-sm-12  col-md-6 ">
        <div class="card bg-transparent shadow " style="border-radius: 1rem;border: none; ;">
          <div class="card-body   bg-white " >
            <div class="mb-md-5 mt-md-4 pb-md-5">
              <h2 class="fw-bold mb-2 text-center text-uppercase text-black pb-4"> Login </h2>
              <% if(typeof message !=='undefined' ){ %>
                <span class="ps-5">
                  <%= message %>
                </span>
                <% } %>
                  <div class="toast-container position-fixed top-50 end-30 p-3">
                  </div>
                  <form action="" method="post">
                    <!-- Email input -->
                    <div class="form-outline mb-4 ps-md-4 pe-md-4">
                      <input type="text" id="username" name="username" class="form-control form-control-lg"
                        placeholder="Username" required />
                      <span id="Email"></span>
                    </div>
                    <!-- Password input -->
                    <div class="form-outline mb-4 ps-md-4 pe-md-4">
                      <input type="password" id="password" class="form-control form-control-lg " name="password"
                        placeholder="Password" required />
                      <span id="Password"></span>
                    </div>
                    <div class="row d-flex justify-content-center">
                      <input type="submit" class="btn btn-secondary text-center w-25" value="Login" />
                    </div>
                  </form>
                  <div class="text-center p-md-1">
                  </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"
